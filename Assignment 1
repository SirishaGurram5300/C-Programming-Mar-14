#include <stdio.h>
int main(void) 
{
     int t;
    scanf("%d",&t);//2
    while(t--)//while(2--)
    {
         long int n, i,sum=0;
        scanf("%ld",&n);//4
        long int a[n];
        for(i=0;i<n;i++)
        scanf("%ld",&a[i]);
        for(i=0;i<n;i++)
        sum=sum+a[i];
        for(i=0;i<n;i++) 
         printf("%ld\n",sum-a[i]);
    }
return 0;
}
